{
    "name": "hgabka/utils-bundle",
    "type": "symfony-bundle",
    "description": "Utils bundle",
    "keywords": [
    ],
    "homepage": "https://github.com/hgabka/utils-bundle",
    "license": "proprietary",
    "authors": [
        {
            "name": "Gabe",
            "email": "hgabka@gmail.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.3 || ^8.0",
        "symfony/framework-bundle": "5.4.*",
        "symfony/acl-bundle": "^2.0",
        "liip/imagine-bundle": "^2.1",
        "hgabka/doctrine-translatable-bundle": "^1.0",
        "google/apiclient": "^2.7",
        "guzzlehttp/guzzle": ">= 6.3"
    },
    "require-dev": {
        "phpunit/phpunit" : "4.*",
        "mockery/mockery": "^0.9.4"
    },
    "autoload": {
        "psr-4": {
            "Hgabka\\UtilsBundle\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Hgabka\\UtilsBundle\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "phpunit"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    }
}
